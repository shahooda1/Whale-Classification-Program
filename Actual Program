""")

    go = input("Press enter to start the program.\n\n")


def actualThing():

    while True:

# Clearing the console so it doesn't get cluttered
        clearConsole()

# Asking the user which entry they want to see

        print("1. Blue Whale")
        print("2. Sperm Whale")
        print("3. Bottlenose Dolphin")
        print("4. Common Dolphin")
        print("5. Orca")
        print("6. Narwhal")
        print("7. Pilot Whale")
        print("8. Fin Whale")
        print("9. Humpback Whale")
        print("10. Beluga Whale")
        print("11. Harbor Porpoise")
        print("12. Amazon River Dolphin")

        entryNum = input("\nEnter a number from 1-12 to view the corresponding entry:\n")


        while entryNum.strip().isdigit() == False or int(entryNum.strip()) < 1 or int(entryNum.strip()) > 12:
            entryNum = input("Please provide a number from 1-12:\n")

# Converting entryNum to an integer
        entryNum = int(entryNum.strip())

# Entry options
        if entryNum == 1:
            entry1()
        elif entryNum == 2:
            entry2()
        elif entryNum == 3:
            entry3()
        elif entryNum == 4:
            entry4()
        elif entryNum == 5:
            entry5()
        elif entryNum == 6:
            entry6()
        elif entryNum == 7:
            entry7()
        elif entryNum == 8:
            entry8()
        elif entryNum == 9:
            entry9()
        elif entryNum == 10:
            entry10()
        elif entryNum == 11:
            entry11()
        elif entryNum == 12:
            entry12()
        

# Option to exit the program
        
        while True:

            endProgram = input("Would you like to see another entry? (y/n):\n").strip().upper()

            if endProgram == 'Y':

                print("\nRunning the program again.")
                break

            elif endProgram == 'N':

                print("Exiting the program.")
                break

            else:

                print("Please enter 'Y' or 'N'.")
                continue
        
        if endProgram == 'N':

            break




mainMenu()

actualThing()
